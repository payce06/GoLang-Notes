package main

import "fmt"

// 1. Function to check if a number is positive, negative, or zero.
func checkNumber(num int) string {
    if num > 0 {
        return "Positive"
    } else if num < 0 {
        return "Negative"
    }
    return "Zero"
}

// 2. Function to find the largest of three numbers.
func findLargestOfThree(a, b, c int) int {
    if a > b && a > c {
        return a
    } else if b > c {
        return b
    }
    return c
}